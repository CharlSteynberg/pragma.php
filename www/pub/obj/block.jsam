
   ({'text/html':
   [
      {h4:(this.$title || this.title)},

      switch
      ([
         {
            where:(this.cast == 'text');
            yield:{p:(this.src)}
         },
         {
            where:(this.cast == 'code');
            yield:{pre:(this.src)}
         },
         {
            where:(this.cast == null);
            yield:{div:(this.src)}
         },
      ])
   ]})
